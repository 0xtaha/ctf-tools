#!/bin/bash -ex

mkdir bin

git clone https://github.com/mentebinaria/elfparser-ng elfparser-ng
mkdir qt
cd qt
cmake ../elfparser-ng
make -j
mv elfparser-ng ../bin/elfparser-gui-ng
cd ..

mkdir cli
cd cli
cmake -D qt=no ../elfparser-ng
make -j
mv elfparser-cli-ng ../bin/
cd ..

rm -rf cli qt elfparser-ng
